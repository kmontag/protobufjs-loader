{
  "name": "protobufjs-loader",
  "version": "0.0.0-development",
  "description": "Webpack loader to translate .proto definitions to ProtoBuf.js modules",
  "main": "index.js",
  "scripts": {
    "check": "tsc",
    "fix:script": "eslint --fix .",
    "fix:style": "prettier --write .",
    "fix": "npm run fix:script && npm run fix:style",
    "lint:script": "eslint .",
    "lint:style": "prettier -c .",
    "lint": "npm run lint:script && npm run lint:style",
    "test": "mocha test/*.test.js"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "webpack",
    "protobuf",
    "protobufjs",
    "ProtoBuf.js"
  ],
  "author": "Kevin Montag <kmontag@cs.stanford.edu>",
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^2.0.2",
    "protobufjs": "^7.0.0",
    "protobufjs-cli": "^1.0.0",
    "schema-utils": "^4.0.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/glob": "^7.2.0",
    "@types/loader-utils": "^2.0.3",
    "@types/memory-fs": "^0.3.3",
    "@types/mocha": "^9.1.1",
    "@types/tmp": "^0.2.3",
    "@types/webpack2": "npm:@types/webpack@^2.0.0",
    "@types/webpack3": "npm:@types/webpack@^3.0.0",
    "@types/webpack4": "npm:@types/webpack@^4.0.0",
    "chai": "^4.1.2",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.0.5",
    "glob": "^7.2.3",
    "inspect-loader": "^1.0.0",
    "memory-fs": "^0.5.0",
    "mocha": "^9.2.2",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "semver": "^7.1.2",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.2",
    "webpack": "^5.0.0",
    "webpack2": "npm:webpack@^2.0.0",
    "webpack3": "npm:webpack@^3.0.0",
    "webpack4": "npm:webpack@^4.0.0"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kmontag/protobufjs-loader.git"
  }
}
